  },
  "dependencies": {
    "@apollo/client": "^3.0.0-rc.5",
    "@apollo/react-common": "3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "antd": "^4.3.1",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.16",
    "axios": "^0.19.2",
    "cross-env": "^7.0.2",
    "customize-cra": "^0.9.1",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.1",
    "http2": "^3.3.7",
