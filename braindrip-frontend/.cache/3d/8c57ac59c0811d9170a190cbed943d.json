l":{"line":69,"column":25}},{"generated":{"line":144,"column":12},"source":"components/Root/Login/Login.js","original":{"line":69,"column":33}},{"name":"isSubmitting","generated":{"line":144,"column":14},"source":"components/Root/Login/Login.js","original":{"line":69,"column":35}},{"generated":{"line":144,"column":26},"source":"components/Root/Login/Login.js","original":{"line":69,"column":12}},{"generated":{"line":145,"column":0},"source":"components/Root/Login/Login.js","original":{"line":69,"column":49}},{"generated":{"line":145,"column":4},"source":"components/Root/Login/Login.js","original":{"line":69,"column":49}},{"generated":{"line":145,"column":8},"source":"components/Root/Login/Login.js","original":{"line":69,"column":53}},{"generated":{"line":145,"column":10},"source":"components/Root/Login/Login.js","original":{"line":69,"column":54}},{"generated":{"line":146,"column":0},"source":"components/Root/Login/Login.js","original":{"line":69,"column":12}},{"generated":{"line":146,"column":13},"source":"components/Root/Login/Login.js","original":{"line":59,"column":8}},{"generated":{"line":146,"column":14},"source":"components/Root/Login/Login.js","original":{"line":58,"column":4}},{"generated":{"line":147,"column":0},"source":"components/Root/Login/Login.js","original":{"line":74,"column":1}},{"generated":{"line":147,"column":1},"source":"components/Root/Login/Login.js","original":{"line":42,"column":0}},{"name":"Login","generated":{"line":149,"column":15},"source":"components/Root/Login/Login.js","original":{"line":76,"column":15}}],"sources":{"components/Root/Login/Login.js":"import { useMutation } from \"@apollo/react-hooks\";\r\nimport gql from \"graphql-tag\";\r\nimport React from 'react';\r\nimport {useForm} from \"react-hook-form\";\r\n\r\nimport styled from 'styled-components';\r\n\r\nimport TextInput from \"#root/components/shared/TextInput\";\r\n\r\n// note REM units stand for relative to root element font-size.\r\nconst Label = styled.label`\r\n    display: block;\r\n\r\n    :not(:first-child) {\r\n        margin-top: .75rem;\r\n    }\r\n`;\r\n\r\nconst LabelText = styled.strong`\r\n    display: block;\r\n    font-size: .9rem;   \r\n    margin-bottom: .25rem;\r\n`;\r\n\r\nconst LoginButton = styled.button`\r\n    display: inline-block;\r\n    margin-top: .5rem;\r\n`;\r\n\r\nconst mutation = gql`\r\n    mutation($email: String!, $password: String!) {\r\n        createUserSession(email: $email, password: $password) {\r\n            id\r\n            user {\r\n                email\r\n                id\r\n            }\r\n        }\r\n    }\r\n`;\r\n\r\nconst Login = () => {\r\n    const [createUserSession] = useMutation(mutation);\r\n    const {\r\n        formState: { isSubmitting },\r\n        handleSubmit,\r\n        register\r\n    } = useForm();\r\n\r\n    const onSubmit = handleSubmit(async ({email,  password}) => {\r\n        const result = await createUserSession({ variables: {\r\n            email, password\r\n        }})\r\n\r\n        console.log(result);\r\n    })\r\n\r\n    return (\r\n        <form onSubmit={onSubmit}>\r\n            <Label>\r\n                <LabelText>Email</LabelText>\r\n                <TextInput disabled={isSubmitting} name=\"email\" type=\"email\" ref={register} />\r\n            </Label>\r\n            {/* Remember, we use the label tag so it is associated with the input */}\r\n            <Label>\r\n                <LabelText>Password</LabelText>\r\n                <TextInput disabled={isSubmitting} name=\"password\" type=\"password\" ref={register} />\r\n            </Label>\r\n            <LoginButton disabled={isSubmitting} type=\"submit\">\r\n                Login\r\n            </LoginButton>\r\n        </form>\r\n    )\r\n}\r\n\r\nexport default Login\r\n"},"lineCount":null}},"error":null,"hash":"738dc42c3a4e0315da1a210c8f98e523","cacheData":{"env":{}}}